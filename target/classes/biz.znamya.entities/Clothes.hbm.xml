<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="biz.znamya.entities.Clothes" table="clothes" schema="znamyadb">
        <id name="id">
            <column name="id" sql-type="bigint(20)"/>
        </id>
        <property name="view">
            <column name="view" sql-type="varchar(300)" length="300"/>
        </property>
    </class>
</hibernate-mapping>